<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:p="http://www.springframework.org/schema/p"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="studentbean1" class="com.demo.Student">
        <property name="studentId" value="101"></property>
        <property name="studentName" value="Abhinav Pandey"></property>
    </bean>

    <bean id="studentbean2" class="com.demo.Student">
        <property name="studentId" value="102"></property>
        <property name="studentName" value="Astha Gupta"></property>
    </bean>
</beans>